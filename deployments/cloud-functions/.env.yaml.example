# Google Cloud Functions 環境変数設定例
# このファイルをコピーして .env.yaml を作成してください

DISCORD_BOT_TOKEN: "your_discord_bot_token_here"
DISCORD_PROFILE_CHANNEL: "your_profile_channel_id_here"
DISCORD_GUILD_ID: "your_jyogi_server_id_here"
DISCORD_CLIENT_ID: "your_discord_client_id_here"
DISCORD_CLIENT_SECRET: "your_discord_client_secret_here"
DISCORD_REDIRECT_URI: "https://your-domain.com/auth/callback"
JWT_SECRET: "your_jwt_secret_minimum_32_characters_long"
DATABASE_PATH: "/tmp/jyogi_auth.db"
ENV: "production"
